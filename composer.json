{
    "name": "panchania83/nova-google-analytics",
    "description": "Google Analytics integration with Nova",
    "keywords": [
        "laravel",
        "nova",
        "tighten",
        "nova-google-analytics",
        "php"
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "homepage": "https://github.com/panchania83/nova-google-analytics",
    "license": "MIT",
    "authors": [
        {
            "name": "Matt Stauffer",
            "email": "matt@tighten.co",
            "role": "Developer"
        },
        {
            "name": "Kristin Collins",
            "email": "kristin.collins@tighten.co",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "spatie/laravel-analytics": "^4.1"
    },
    "require-dev": {
       "laravel/nova": "*",
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.5",
        "tightenco/tlint": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "Tightenco\\NovaGoogleAnalytics\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tightenco\\NovaGoogleAnalytics\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tightenco\\NovaGoogleAnalytics\\ToolServiceProvider",
                "Tightenco\\NovaGoogleAnalytics\\CardServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "post-autoload-dump": [
            "@php vendor/bin/testbench package:discover --ansi"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
